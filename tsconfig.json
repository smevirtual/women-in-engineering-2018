{
  "compilerOptions": {
    "declaration": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
